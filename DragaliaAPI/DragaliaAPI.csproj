	<ItemGroup>
		<ProjectReference Include="..\DragaliaAPI.Database\DragaliaAPI.Database.csproj" />
		<ProjectReference Include="..\DragaliaAPI.Photon.Shared\DragaliaAPI.Photon.Shared.csproj" />
